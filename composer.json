{
    "name": "ardc/lst-agldwg",
    "description": "LinkStitcher AGLDWG",
    "type": "drupal-custom-module",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Richard Walker",
            "email": "Richard.Walker@ardc.edu.au"
        }
    ],
    "require": {
        "cweagans/composer-patches": "~1.0",
        "drupal/backup_migrate": "^5.0.1",
        "drupal/ctools": "^3.4",
        "drupal/field_group": "^3.1",
        "drupal/pathauto": "^1.8",
        "drupal/field_permissions": "^1.1",
        "drupal/diff": "^1.0",
        "drupal/structure_sync": "^2.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/coi": "^3.0",
        "drush/drush": "^10.3",
        "drupal/mimemail": "^1.0",
        "drupal/typed_data": "^1.0",
        "drupal/rules": "^3.0",
        "drupal/tr_rulez": "^1.0",
        "drupal/require_revision_log_message": "^1.0",
        "drupal/contact_storage": "^1.1",
        "drupal/readonlymode": "^1.1",
        "drupal/token_filter": "^1.2",
        "drupal/eme": "1.0.0-alpha10",
        "ardc/lst-agldwg-theme": ">=1.1",
        "ardc/lst-agldwg-block-content": ">=1.1"
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "On tracker pages, add space between link and status": "web/modules/custom/lst-agldwg/patches/drupal/core/3095533-18.patch.txt",
                "Improve styling for select and add styling to select[multiple]": "web/modules/custom/lst-agldwg/patches/drupal/core/2491725-31-multiple-select.patch.txt",
                "Image styles fails with absolute path in $settings['file_public_path']": "web/modules/custom/lst-agldwg/patches/drupal/core/image_styles-new.patch"

            },
            "drush/drush": {
                "Support utf8mb4 on drush site-install": "web/modules/custom/lst-agldwg/patches/drush/drush/4810.patch.txt"
            },
            "drupal/backup_migrate": {
                "Don't try to read directory entries as files during backup": "web/modules/custom/lst-agldwg/patches/drupal/backup_migrate/bam_empty_dirs.patch.txt"
            },
            "drupal/rules": {
                "Integrate Typed Data Widgets": "web/modules/custom/lst-agldwg/patches/drupal/rules/2471481-146.integrate-typed-data-widgets.patch.txt",
                "Support upcasting entity IDs to full entity contexts": "web/modules/custom/lst-agldwg/patches/drupal/rules/2800749-88.upcast.patch.txt"
            },
            "drupal/contact_storage": {
                "Error while field storage definition": "web/modules/custom/lst-agldwg/patches/drupal/contact_storage/3053497-8.patch.txt"
            },
            "drupal/readonlymode": {
                "Got fatal error when running this module: string, not array": "web/modules/custom/lst-agldwg/patches/drupal/readonlymode/readonlymode-string_not_array-3115119-9.patch.txt"
            },
            "drupal/eme": {
                "Optionally allow exporting all entity revisions, not just the default one and the default translations": "web/modules/custom/lst-agldwg/patches/drupal/eme/eme-alpha10-with-revisions-3.patch.txt"
            }
        }
    }
}
